{
  "name": "my-trips",
  "version": "1.0.0",
  "description": "A website for travelers to organize their trip details.",
  "main": "server/index.js",
  "scripts": {
    "test": "mocha",
    "tdd": "mocha --watch -R nyan",
    "start": "npm run bundle && node .",
    "watch": "npm run bundle:watch & nodemon .",
    "migrate:make": "knex migrate:make",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback",
    "bundle": "browserify -t babelify src/ -o public/bundle.js",
    "bundle:watch": "watchify -t babelify src/ -o public/bundle.js",
    "envision": "envision src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeshicawang/MyTrips.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jeshicawang/MyTrips/issues"
  },
  "homepage": "https://github.com/jeshicawang/MyTrips#readme",
  "dependencies": {
    "babelify": "^7.3.0",
    "body-parser": "^1.15.2",
    "browserify": "^14.1.0",
    "express": "^4.14.0",
    "knex": "^0.12.7",
    "moment": "^2.17.1",
    "pg": "^6.1.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "sqlite3": "^3.1.8"
  },
  "devDependencies": {
    "babel-plugin-transform-react-jsx": "^6.22.0",
    "babelify": "^7.3.0",
    "browserify": "^14.0.0",
    "chai": "^3.5.0",
    "envision-jsx": "^1.0.8",
    "eslint": "^3.14.1",
    "eslint-plugin-react": "^6.9.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "watchify": "^3.9.0"
  },
  "engines": {
    "node": "6.9.2",
    "npm": "3.10.9"
  }
}
